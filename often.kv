<ShopScr>:
    FloatLayout:
        Image:
            source: 'D:\\Projects\\pycharmprojects\\1772.jpg'
        BoxLayout:
            orientation: 'vertical'
            ScrollView:
                size_hint_y: None
                size_hint: 1, 1
                GridLayout:
                    id: items
                    size_hint_y: None
                    height: self.minimum_height
                    cols: 1
                    spacing: 30
                    row_default_height: 100
                    FloatLayout:
                        Button:
                            size_hint: 1, 1
